{
    "name": "vendor-name/my-custom-api-plugin",
    "description": "Fetches data from external API and shows a HTML table of users when a custom-endpoint is visited.",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Ivan Frolyak Ostrovskyy",
            "email": "osy.ivan@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "inpsyde/php-coding-standards": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Frolyak\\FrolyakIvanWpPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frolyak\\FrolyakIvanWpPlugin\\Tests\\Unit\\": "tests/PHPUnit/Unit"
        }
    },
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
